{
  "name": "octopus-deploy",
  "version": "4.1.4",
  "description": "Node scripts to package up applications, create releases, and deploy with Octopus Deploy. This package leverages the Octopus Deploy REST API in order to deploy from Windows and non-Windows machines.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "test": "mocha",
    "test:ci": "npm run test -- --reporter tap",
    "test:cover": "nyc npm run test",
    "test:watch": "npm run test -- --watch"
  },
  "bin": {
    "octopus-deploy-create-release": "./bin/create-release.js",
    "octopus-deploy-create-release-and-deploy": "./bin/create-release-and-deploy.js",
    "octopus-deploy-pack-and-push": "./bin/pack-and-push.js"
  },
  "engines": {
    "node": ">=6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parkerholladay/node-octopus-deploy.git"
  },
  "keywords": [
    "cli",
    "cross-platform",
    "deploy",
    "linux",
    "node",
    "octo",
    "octopack",
    "octopus",
    "pack",
    "package"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parkerholladay/node-octopus-deploy/issues"
  },
  "homepage": "https://github.com/parkerholladay/node-octopus-deploy",
  "dependencies": {
    "archiver": "3.0.0",
    "bluebird": "3.5.5",
    "glob-stream": "6.1.0",
    "json-casing": "1.1.8",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "yargs": "13.2.4"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.4.1",
    "mocha": "6.1.4",
    "nyc": "14.1.1",
    "sinon": "7.3.2",
    "sinon-chai": "3.3.0"
  }
}
